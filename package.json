{
  "name": "every-test",
  "version": "0.0.1",
  "description": "Super-simple DOM testing library",
  "main": "react.js",
  "author": "Reid Mitchell",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "build": "rm -rf dist && tsc"
  },
  "dependencies": {
    "pretty-format": "^23.6.0",
    "sizzle": "^2.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@types/jest": "^23.3.5",
    "@types/pretty-format": "^20.0.0",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/sizzle": "^2.3.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.7.0",
    "jest": "^23.6.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "ts-jest": "^23.10.4",
    "typescript": "^3.1.3"
  },
  "jest": {
    "roots": [
      "test"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageReporters": [
      "html"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
